
	<!-- begin:heading -->
	<div class="heads" style="background: url(img/img02.jpg) center center;">
		<div class="container">
			<div class="row">
				<div class="col-md-12">
					<h2><span>//</span> MemberShip Plans</h2>
				</div>
			</div>
		</div>
	</div>
	<!-- end:heading -->
	
	<!-- begin:pricing -->
	<div class="page-content contact">
		<div class="container">
			<div class="row">
				<div class="col-md-12">
					<ol class="breadcrumb">
				      <li><a href="/">Home</a></li>
				      <li class="active">Pricing</li>
				    </ol>
				</div>
			</div>
			
			<div class="row">
				<div class="col-md-12 text-center">
					<h3>Become a Member</h3>
					<p>Here are the plans for membership.</p>
				</div>
			</div>
			
            {{#each plans}}
			<div class="row padd20-top-btm">
				<div class="col-md-12 text-center">
					<h2>Basic Membership</h2>
				</div>
			</div>
			<div class="row">
            {{#each this}} 
				<div class="col-md-3 col-sm-6 col-xs-12 pricing-table">
		            <ul class="list-unstyled">
		              <li class="head"></li>
		              <li class="price"><sup>&#x20b9;</sup>{{amount}} <p>{{duration}}</p><p></p></li>
		              <li>Rent {{noOfToys}} Toy(s) for {{rentDuration}}</li>
		              <li>With Deposite of <sup>&#x20b9;</sup>{{deposit}} <p></p></li>
		              <li class="foot"><a href="/subscribePlan?type={{@../index}}&plan={{@index}}" class="btn btn-green btn-larges">Subscribe</a></li>
		            </ul>
		        </div>
			
        {{/each}}
            </div>
        {{/each}}
	</div>
	<!-- end:pricing -->
</div>
